package level2;

import java.util.Scanner;

public class Dise {

	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		int a, b, c, money;
		a = sc.nextInt();
		b = sc.nextInt();
		c = sc.nextInt();
		
		if (a != b && b !=c && a != c) {
			// 가장 큰 수 반환
		}else if (a==b || b==c || a == c){
			if (a==b) {
				money = 1000 + (a * 100);
			}else if (b == c) {
				money = 1000 + (b * 100);
			}else {
				money = 1000 + (c * 100);
			}
			if(a == b && b == c && a == c) {
				money = 1000 + (a * 1000);
			}
		}

	}

}
